#include<iostream>
using namespace std;

#define SIZE 100

class Queue{
	int arr[SIZE];
	int front;
	int rear;
	
	public:
		Queue(){
			front=-1;
			rear=-1;
		}
		
		void enqueue(int x){
			if(rear==SIZE-1){
				cout<<"Queue overflow";
			}else if(front==-1 && rear==-1){
				front=rear=0;
				arr[rear]=x;
			}else{
				rear++;
				arr[rear]=x;
			}
		}
		
		void dequeue(){
			if(front==-1 && rear==-1){
				cout<<"Queue underflow";
			}else if(front==rear){
				front=rear=-1;
			}else{
				cout<<"Dequeued element"<<arr[front];
				front++;
			}
		}
		
		
		void display(){
			for(int i=front;i<=rear;i++){
				cout<<arr[i]<<" ";
			}
		}
};



int main() {
    Queue q;
    int choice, value;

    do {
        cout << "\n1.Enqueue  2.Dequeue  3.Display  4.Exit\n";
        cout << "Enter your choice: ";
        cin >> choice;

        switch (choice) {
            case 1:
                cout << "Enter value to enqueue: ";
                cin >> value;
                q.enqueue(value);
                break;
            case 2:
                q.dequeue();
                break;
            case 3:
                q.display();
                break;
            case 4:
                cout << "Exiting...\n";
                break;
            default:
                cout << "Invalid choice!\n";
        }
    } while (choice != 4);

    
}
