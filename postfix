#include<iostream>
using namespace std;
#define SIZE 100
class Stack{
	int arr[SIZE];
	int top;
	
	public:
		Stack(){
			top=-1;
		}
		
		void push(int x){
			if(top==SIZE-1){
				cout<<"Stack overflow";
			}else{
				top++;
				arr[top]=x;
			}
		}
		
		int pop(){
			if(top==-1){
				cout<<"Stack underflow";
				return -1;
			}else{
				int popVal=arr[top];
				top--;
				return popVal;
			}
		}
};


int evaluatePostfix(string str){
	Stack s;
	for(int i=0;i<str.length();i++){
		char ch = str[i];
		
		if(isdigit(ch)){
			s.push(ch-'0');
		}else{
			int b=s.pop();
			int a=s.pop();
			
			int result;
			
			switch(ch){
				case '+':
					result=a+b;
					break;
				case '-':
					result=a-b;
					break;
				case '*':
					result=a*b;
					break;
				case '/':
					result=a/b;
					break;
			}
			s.push(result);
		}
	}
	return s.pop();
}



int main(){
	string str;
	
	cout<<"Enter the expression:";
	cin>>str;
	
	int r = evaluatePostfix(str);
	cout<<"Value is "<<r;
}



